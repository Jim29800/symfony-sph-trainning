<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\UtiUtilisateur" table="uti_utilisateur">
    <unique-constraints>
      <unique-constraint name="uti_pseudo" columns="uti_pseudo"/>
    </unique-constraints>
    <id name="utiOid" type="integer" column="uti_oid">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="utiPseudo" type="string" column="uti_pseudo" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="utiNom" type="string" column="uti_nom" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="utiPrenom" type="string" column="uti_prenom" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="utiMdp" type="string" column="uti_mdp" length="250" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="utiAutorisation" type="integer" column="uti_autorisation" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
