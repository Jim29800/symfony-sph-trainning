<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\CliClient" table="cli_client">
    <id name="cliOid" type="integer" column="cli_oid">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="cliNom" type="string" column="cli_nom" length="250" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="cliPrenom" type="string" column="cli_prenom" length="250" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="cliEmail" type="string" column="cli_email" length="250" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="cliAdresse" type="string" column="cli_adresse" length="500" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="cliCp" type="string" column="cli_cp" length="5" nullable="true">
      <options>
        <option name="fixed">1</option>
      </options>
    </field>
    <field name="cliVille" type="string" column="cli_ville" length="100" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="cliTel" type="string" column="cli_tel" length="12" nullable="true">
      <options>
        <option name="fixed">1</option>
      </options>
    </field>
    <field name="cliCommentaire" type="text" column="cli_commentaire" length="65535" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="cliProvenance" type="string" column="cli_provenance" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
